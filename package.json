{
  "name": "react-airsonic",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && VITE_COMMIT_SHA=`git rev-parse --short HEAD` vite build",
    "coverage": "vitest run --coverage",
    "dev": "cross-env VITE_COMMIT_SHA=dev vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest --ui --coverage"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "eslint --fix",
    "package.json": "npx sort-package-json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "@react-hook/window-size": "^3.1.1",
    "@tanstack/react-query": "^5.51.9",
    "@tanstack/react-query-devtools": "^5.51.21",
    "@tanstack/react-router": "^1.47.1",
    "@tanstack/react-virtual": "^3.2.1",
    "@tanstack/router-devtools": "^1.47.1",
    "axios": "^1.7.3",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "i18next": "^23.11.1",
    "i18next-resources-to-backend": "^1.2.1",
    "md5": "^2.3.0",
    "react": "^19.0.0-rc.0",
    "react-dom": "^19.0.0-rc.0",
    "react-focus-lock": "^2.11.2",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^15.0.0",
    "react-modal": "^3.16.1",
    "react-tooltip": "^5.26.3",
    "slugify": "^1.6.6",
    "temporal-polyfill": "^0.2.4",
    "typescript": "^5.4.5",
    "uuid": "^10.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.4.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/types": "19.0.3",
    "@eslint/js": "^9.8.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@tanstack/router-vite-plugin": "^1.58.4",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint__js": "^8.42.3",
    "@types/md5": "^2.3.5",
    "@types/node": "^22.1.0",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@types/react-modal": "^3.16.3",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.13",
    "postcss": "^8.4.38",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.6",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.0.1",
    "unplugin-inject-preload": "^2.0.4",
    "vite": "^5.4",
    "vite-plugin-mkcert": "^1.17.5",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.0"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.23.0",
    "@esbuild/linux-x64": "^0.23.0",
    "@rollup/rollup-darwin-arm64": "^4.20.0",
    "@rollup/rollup-linux-x64-gnu": "^4.17.2"
  },
  "overrides": {
    "jsdom": {
      "tough-cookie": "next"
    },
    "msw": {
      "typescript": "$typescript"
    },
    "@heroicons/react": {
      "react": "$react"
    },
    "@react-hook/window-size": {
      "react": "$react"
    },
    "@tanstack/react-query": {
      "react": "$react"
    },
    "@tanstack/react-query-devtools": {
      "react": "$react"
    },
    "@tanstack/react-router": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@tanstack/router-devtools": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@tanstack/react-virtual": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@testing-library/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-focus-lock": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-helmet-async": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-i18next": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-modal": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-tooltip": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@react-hook/latest": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-clientside-effect": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
