{
  "name": "react-airsonic",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && VITE_COMMIT_SHA=`git rev-parse --short HEAD` vite build",
    "dev": "cross-env VITE_COMMIT_SHA=dev vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "eslint --fix",
    "package.json": "npx sort-package-json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@react-hook/window-size": "^3.1.1",
    "@tanstack/react-query": "^5.22.2",
    "@tanstack/react-query-devtools": "^5.24.0",
    "@tanstack/react-router": "^1.16.6",
    "@tanstack/react-virtual": "^3.1.2",
    "@tanstack/router-devtools": "^1.16.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.3.1",
    "i18next": "^23.10.0",
    "i18next-resources-to-backend": "^1.2.0",
    "md5": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.11.1",
    "react-helmet-async": "^2.0.4",
    "react-i18next": "^14.0.5",
    "react-modal": "^3.16.1",
    "react-tooltip": "^5.26.3",
    "temporal-polyfill": "^0.2.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "web-vitals": "^3.5.2",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@commitlint/types": "18.6.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@tanstack/router-vite-plugin": "^1.16.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.59",
    "@types/react-dom": "^18.2.19",
    "@types/react-modal": "^3.16.3",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "jest-localstorage-mock": "^2.4.26",
    "lint-staged": "^15.2.2",
    "msw": "^2.2.1",
    "postcss": "^8.4.35",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.1",
    "ts-node": "^10.9.2",
    "vite": "^5.1",
    "vite-plugin-pwa": "^0.19.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "^0.20.1",
    "@rollup/rollup-linux-x64-gnu": "^4.12.0"
  },
  "overrides": {
    "msw": {
      "typescript": "$typescript"
    }
  }
}
